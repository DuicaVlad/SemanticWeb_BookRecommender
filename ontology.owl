<?xml version="1.0"?>
<rdf:RDF xmlns="http://example.org/bookstore/ontology#"
         xml:base="http://example.org/bookstore/ontology"
         xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
         xmlns:owl="http://www.w3.org/2002/07/owl#"
         xmlns:xml="http://www.w3.org/XML/1998/namespace"
         xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
         xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#">
    
    <owl:Ontology rdf:about="http://example.org/bookstore/ontology"/>
    
    <owl:Class rdf:about="#User">
        <rdfs:comment>A person who reads books.</rdfs:comment>
    </owl:Class>
    
    <owl:Class rdf:about="#Book">
        <rdfs:comment>A written work published in printed or electronic form.</rdfs:comment>
    </owl:Class>
    
    <owl:ObjectProperty rdf:about="#recommends">
        <rdfs:domain rdf:resource="#User"/>
        <rdfs:range rdf:resource="#Book"/>
    </owl:ObjectProperty>
    
    <owl:DatatypeProperty rdf:about="#hasName">
        <rdfs:domain rdf:resource="#User"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    </owl:DatatypeProperty>
    
    <owl:DatatypeProperty rdf:about="#readingLevel">
        <rdfs:domain rdf:resource="#User"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    </owl:DatatypeProperty>
    
    <owl:DatatypeProperty rdf:about="#prefersTheme">
        <rdfs:domain rdf:resource="#User"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    </owl:DatatypeProperty>
    
    <owl:DatatypeProperty rdf:about="#hasTitle">
        <rdfs:domain rdf:resource="#Book"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    </owl:DatatypeProperty>
    
    <owl:DatatypeProperty rdf:about="#hasAuthor">
        <rdfs:domain rdf:resource="#Book"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    </owl:DatatypeProperty>
    
    <owl:DatatypeProperty rdf:about="#hasTheme">
        <rdfs:domain rdf:resource="#Book"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    </owl:DatatypeProperty>
    
    <owl:DatatypeProperty rdf:about="#suitableForLevel">
        <rdfs:domain rdf:resource="#Book"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    </owl:DatatypeProperty>
    
    <owl:NamedIndividual rdf:about="#Alice">
        <rdf:type rdf:resource="#User"/>
        <hasName>Alice</hasName>
        <readingLevel>Intermediate</readingLevel>
        <prefersTheme>Science Fiction</prefersTheme>
    </owl:NamedIndividual>
    
    <owl:NamedIndividual rdf:about="#Bob">
        <rdf:type rdf:resource="#User"/>
        <hasName>Bob</hasName>
        <readingLevel>Beginner</readingLevel>
        <prefersTheme>Mystery</prefersTheme>
    </owl:NamedIndividual>
    
    <owl:NamedIndividual rdf:about="#Dune">
        <rdf:type rdf:resource="#Book"/>
        <hasTitle>Dune</hasTitle>
        <hasAuthor>Frank Herbert</hasAuthor>
        <hasTheme>Science Fiction</hasTheme>
        <hasTheme>Fantasy</hasTheme>
        <suitableForLevel>Advanced</suitableForLevel>
    </owl:NamedIndividual>
    
    <owl:NamedIndividual rdf:about="#HungerGames">
        <rdf:type rdf:resource="#Book"/>
        <hasTitle>Hunger Games</hasTitle>
        <hasAuthor>Suzanne Collins</hasAuthor>
        <hasTheme>Science Fiction</hasTheme>
        <hasTheme>Fantasy</hasTheme>
        <suitableForLevel>Beginner</suitableForLevel>
    </owl:NamedIndividual>
    
    <owl:NamedIndividual rdf:about="#TheSilentPatient">
        <rdf:type rdf:resource="#Book"/>
        <hasTitle>The Silent Patient</hasTitle>
        <hasAuthor>Alex Michaelides</hasAuthor>
        <hasTheme>Mystery</hasTheme>
        <hasTheme>Murder</hasTheme>
        <suitableForLevel>Intermediate</suitableForLevel>
    </owl:NamedIndividual>
    
</rdf:RDF>